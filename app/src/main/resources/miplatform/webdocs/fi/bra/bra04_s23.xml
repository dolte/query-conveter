function form_OnLoadCompleted(obj)
{
	gfn_LoadForm(obj);
	
	// 센터코드 가져오기
	gfn_GetDataGIBU_CD(cob_BRAN_CD, 1);

	// 입금구분 가져오기
	gfn_PubCode("ds_REPT_GBN", 2, "00141", "A");
	if (this.ds_REPT_GBN.count > 0) 
	{
		this.ds_REPT_GBN.DeleteRow(7);	//	기타 삭제
		this.ds_REPT_GBN.DeleteRow(6);	//	반환 삭제
	}

	//  기준년도 설정
	var currYear = SubStr(Today(), 0, 4);
	this.sp_YEAR.Value = currYear;
	this.sp_ORG_YEAR.Value = currYear;

	// Grid Header 년도 표시
	this.grd_OrgNote.SetCellProp("head", 1, "text", ToString(ToNumber(currYear)-1));
	this.grd_OrgNote.SetCellProp("head", 2, "text", currYear);
	this.grd_Note.SetCellProp   ("head", 1, "text", ToString(ToNumber(currYear)-1));
	this.grd_Note.SetCellProp   ("head", 2, "text", currYear);

	gfn_SetPermission(cob_BRAN_CD.Value);	
}

