	this.ds_ErrPrint.ClearData();
	for (var i=0; i<this.ds_ReptErr.rowcount; i++) {
		if (this.ds_ReptErr.GetColumn(i, "PRNT_YN") == "1") {
			rIdx = this.ds_ErrPrint.AddRow();

			this.ds_ErrPrint.SetColumn(rIdx, "BRAN_CD"      , this.ds_ReptErr.getColumn(i, "BRAN_CD"      ));
			this.ds_ErrPrint.SetColumn(rIdx, "DEPT_NM"      , this.ds_ReptErr.getColumn(i, "DEPT_NM"      ));
			this.ds_ErrPrint.SetColumn(rIdx, "UPSO_CD"      , this.ds_ReptErr.getColumn(i, "UPSO_CD"      ));
			this.ds_ErrPrint.SetColumn(rIdx, "UPSO_NM"      , this.ds_ReptErr.getColumn(i, "UPSO_NM"      ));
			this.ds_ErrPrint.SetColumn(rIdx, "DEMD_YRMN"    , this.ds_ReptErr.getColumn(i, "DEMD_YRMN"    ));
			this.ds_ErrPrint.SetColumn(rIdx, "START_YRMN"   , this.ds_ReptErr.getColumn(i, "START_YRMN"   ));
			this.ds_ErrPrint.SetColumn(rIdx, "END_YRMN"     , this.ds_ReptErr.getColumn(i, "END_YRMN"     ));
			this.ds_ErrPrint.SetColumn(rIdx, "GRADNM"       , this.ds_ReptErr.getColumn(i, "GRADNM"       ));
			this.ds_ErrPrint.SetColumn(rIdx, "MCHNDAESU"    , this.ds_ReptErr.getColumn(i, "MCHNDAESU"    ));
			this.ds_ErrPrint.SetColumn(rIdx, "MONPRNCFEE"   , this.ds_ReptErr.getColumn(i, "MONPRNCFEE"   ));
			this.ds_ErrPrint.SetColumn(rIdx, "TOT_DEMD_AMT" , this.ds_ReptErr.getColumn(i, "TOT_DEMD_AMT" ));
			this.ds_ErrPrint.SetColumn(rIdx, "TOT_ADDT_AMT" , this.ds_ReptErr.getColumn(i, "TOT_ADDT_AMT" ));
			this.ds_ErrPrint.SetColumn(rIdx, "TOT_EADDT_AMT", this.ds_ReptErr.getColumn(i, "TOT_EADDT_AMT"));
			this.ds_ErrPrint.SetColumn(rIdx, "DEMD_MMCNT"   , this.ds_ReptErr.getColumn(i, "DEMD_MMCNT"   ));
			this.ds_ErrPrint.SetColumn(rIdx, "TRNF_RSLT"    , this.ds_ReptErr.getColumn(i, "TRNF_RSLT"    ));
			this.ds_ErrPrint.SetColumn(rIdx, "CODE_NM"      , this.ds_ReptErr.getColumn(i, "CODE_NM"      ));		
		}
	}
	this.grd_ErrPrint.ExportExcelEx("오류내역");
}

function fn_ProcAutoData()
{
	// 파일 일자, 적용년월 검사
	var fileDay = substr(this.fil_Auto.FileName, indexOf(this.fil_Auto.FileName, "GP24") + 4);

	if (substr(fileDay, 0, 4) != substr(this.cal_PROC_DAY.Value, 2, 4)) 
	{
		if (confirm("적용년월 " + substr(this.cal_PROC_DAY.Text, 0, 7) + ' 이 정확합니까?') == false)
		{
			return;
		}
	}

	// file length check
	var fileSize = this.fil_Auto.GetLength();

	if ( fileSize < 0 || !this.fil_Auto.Open("r")) 
	{
        gfn_Confirm("MB_OK", "화일을 open 하지 못했습니다.!!", "경고", "0");
		return;
	}
	
	var sCont = this.fil_Auto.Read();
	var vArr = sCont.split("\n");

	gfn_SetStatusMsg("자동이체 입금 에러건 처리 진행중....");
	
	this.mae_StartTime.Value = getDate();
	this.mae_EndTime.Value = "";

	// DataSet 초기화
	this.ds_AutoTrailer.ClearData();
	this.ds_ReptResult.ClearData();
	this.ds_AutoBody.ClearData();
	this.ds_ReptAutoSum.ClearData();

	var dsIdx = 0;
	var gbn = "";

	for (var i=0; i<length(vArr); i++)
	{
		gbn = trim(Midb(vArr[i], 0,  2));

		// header 
		if (gbn == "12") {
			this.ds_AutoHeader.AddRow();
			this.ds_AutoHeader.SetColumn(0, "AUTO_GBN", trim(Midb(vArr[i],   0,  2))); // 구분 
			this.ds_AutoHeader.SetColumn(0, "ORG_NM"  , trim(Midb(vArr[i],   2, 34))); // 이용기관명
			this.ds_AutoHeader.SetColumn(0, "INDTN_NO", trim(Midb(vArr[i],  36,  7))); // 지로번호
			this.ds_AutoHeader.SetColumn(0, "BANK_CD" , trim(Midb(vArr[i],  43,  7))); // 거래은행점별코드
			this.ds_AutoHeader.SetColumn(0, "ACCN_NUM", trim(Midb(vArr[i],  50, 15))); // 계좌번호
			this.ds_AutoHeader.SetColumn(0, "ACCN_NM" , trim(Midb(vArr[i],  65, 16))); // 예금주명
			this.ds_AutoHeader.SetColumn(0, "RECV_DAY", trim(Midb(vArr[i],  81,  8))); // 납기일
		}
		// body
		else if (gbn == "22") {
			this.ds_AutoBody.AddRow();
			this.ds_AutoBody.SetColumn(dsIdx, "REPT_DAY"    , '20' + substr(fileDay, 0, 6)); // 등록일
			this.ds_AutoBody.SetColumn(dsIdx, "REPT_YRMN"   , substr(this.cal_PROC_DAY.Value, 0, 6));  // 입금년월
			this.ds_AutoBody.SetColumn(dsIdx, "AUTO_GBN"    , trim(Midb(vArr[i],   0,  2))); // 구분 2
			this.ds_AutoBody.SetColumn(dsIdx, "SEQ_NUM"     , trim(Midb(vArr[i],   2,  7))); // 일련번호 7
			this.ds_AutoBody.SetColumn(dsIdx, "CLIENT_NUM"  , trim(Midb(vArr[i],   9, 20))); // 납부자 번호 20
			this.ds_AutoBody.SetColumn(dsIdx, "RECV_BANK_CD", trim(Midb(vArr[i],  29,  3))); // 납부자 계좌개설 은행코드 3
			this.ds_AutoBody.SetColumn(dsIdx, "ACCN_NUM"    , trim(Midb(vArr[i],  32, 15))); // 계좌번호 15
			this.ds_AutoBody.SetColumn(dsIdx, "RESI_NUM"    , trim(Midb(vArr[i],  47, 13))); // 주민번호 13
			this.ds_AutoBody.SetColumn(dsIdx, "REMAK"       , trim(Midb(vArr[i],  60, 20))); // 출금내역 20
			this.ds_AutoBody.SetColumn(dsIdx, "REPT_AMT"    , trim(Midb(vArr[i],  80, 11))); // 금액 11
			this.ds_AutoBody.SetColumn(dsIdx, "TRNF_RSLT"   , trim(Midb(vArr[i],  91,  2))); // 청구결과 코드 2
			this.ds_AutoBody.SetColumn(dsIdx, "RECPT_CD"    , trim(Midb(vArr[i],  93,  1))); // 접수처 구분코드 1
			this.ds_AutoBody.SetColumn(dsIdx, "INSPRES_ID"  , GV_USER_ID);  				 // 등록자 ID

			dsIdx = dsIdx + 1;
		}
		// trailer
		else if (gbn == "32") {
			this.ds_AutoTrailer.AddRow();
			this.ds_AutoTrailer.SetColumn(0, "DEMD_CNT" , trim(Mid(vArr[i],  2,  7)));    	//청 청구건수 7   
			this.ds_AutoTrailer.SetColumn(0, "DEMD_AMT" , trim(Mid(vArr[i],  9, 13)));    	//총 청구 금액 13  
			this.ds_AutoTrailer.SetColumn(0, "REPT_CNT" , trim(Mid(vArr[i], 22,  7)));    	//전액 출금 건수 7
			this.ds_AutoTrailer.SetColumn(0, "REPT_AMT" , trim(Mid(vArr[i], 29, 13)));    	//전액 출금 금액 13
			this.ds_AutoTrailer.SetColumn(0, "ERR_CNT " , trim(Mid(vArr[i], 42,  7)));    	//출금 불능 건수 7
			this.ds_AutoTrailer.SetColumn(0, "ERR_AMT " , trim(Mid(vArr[i], 49, 13)));    	//출금 불능 금액 13
			this.ds_AutoTrailer.SetColumn(0, "COMIS"    , trim(Mid(vArr[i], 82,  9)));    	//수수료 9
			this.ds_AutoTrailer.SetColumn(0, "FINAL_AMT", trim(Mid(vArr[i], 91, 13)));    	//차감 이체 금액 13
		}
	}
	this.fil_Auto.Close();

	// 오류 내역이 없는 경우의 처리
	if (this.ds_AutoBody.count == 0) 
	{
		this.ds_AutoBody.AddRow();
		this.ds_AutoBody.SetColumn(0, "REPT_DAY"   , '20' + substr(fileDay, 0, 6)); // 등록일
		this.ds_AutoBody.SetColumn(0, "REPT_YRMN"  , substr(this.cal_PROC_DAY.Value, 0, 6));  // 입금년월
		this.ds_AutoBody.SetColumn(0, "INSPRES_ID" , GV_USER_ID);  					// 등록자 ID
	}
	/*
	    로그처리
	*/
	gfn_syncCall("loginfo_select","KOMCA?SYSID=PATHFINDER&MENUID=1000001006007&EVENTID=loginfo_select","S=ds_AutoBody:U S1=ds_AutoHeader:U S2=ds_AutoTrailer:U","","","fn_CallBack",gv_TraceLog,GV_FORMID);

	gfn_AsyncCall("ReptAuto","KOMCA?SYSID=PATHFINDER&MENUID=1000001006001003&EVENTID=rept_auto_insert","S=ds_AutoBody S1=ds_AutoHeader S2=ds_AutoTrailer","ds_ReptAutoSum=SEL2 ds_ReptResult=SEL3 ds_ReptErr=SEL4","","fn_CallBack");

	//trace("===========================================");
	//trace(this.ds_ReptAutoSum.SaveXML());
	//trace("===========================================");
	//trace(this.ds_AutoTrailer.SaveXML());
}



function btn_AutoProc_OnClick(obj)
{
	if (!gfn_CheckFormNull(this)) return;
	
	this.ds_ReptIn.ClearData();
	this.ds_ReptIn.AddRow();
	this.ds_ReptIn.SetColumn(0, "BRAN_CD", this.cal_PROC_DAY.Value );
	gfn_syncCall("rept_closing","KOMCA?SYSID=PATHFINDER&MENUID=1000001006001003&EVENTID=rept_closing","S=ds_ReptInput","ds_closingYn=SEL1","","fn_CallBack");
	
	if(ds_closingYn.GetColumn(0, "CNT") != "0"){
        gfn_Confirm("MB_OK", "해당기간중 마감내역이 존재합니다. 마감내역을 확인하세요.", "경고", "0");
		return;
	}
		
	var sMsg = substr(this.cal_PROC_DAY.Text, 0, 7) + " 에 대한 UPLOAD 진행하시겠습니까? ";
	if (confirm (sMsg) == true)
	{
		fn_ProcAutoData();
	}
	else
	{
		return;
	}
}

function btn_SearchFile_OnClick(obj)
{
	this.fdg_Auto.filter = "청구결과데이터 (GP24) 파일 | GP24*";
	this.fdg_Auto.Open();
	
	if (length(this.fdg_Auto.FilePath) > 0) {
		this.fil_Auto.FileName = this.fdg_Auto.FilePath + "\\" + this.fdg_Auto.FileName;
		this.edt_FileName.Text = this.fil_Auto.FileName;
	
		var fileDay = "20" + substr(this.fil_Auto.FileName, indexOf(this.fil_Auto.FileName, "GP24") + 4);
		this.mae_StartDay.Value = fileDay;
		this.cal_PROC_DAY.Value = fileDay;
	}
}

