function form_OnLoadCompleted(obj)
{
	// 센터코드 가져오기
	gfn_GetDataGIBU_CD(this.cob_BRAN_CD, 0);
	
	if (SGBN == "bra04_r04") {
		this.cob_BRAN_CD.Value = BRAN_CD;
		this.edt_UPSO_NM.Text = UPSO_NM;
		this.edt_MNGEMSTR_NM.Text = MNGEMSTR_NM;
		
		fn_getAccuList();
	}
	else if (SGBN == "bra05_s03") {
		if( length(BRAN_CD)  > 0 ) this.cob_BRAN_CD.Value = BRAN_CD;
		if( length(ACCU_GBN) > 0 ) this.cob_accu_gbn.Value = ACCU_GBN;
		if( length(ACCU_DAY) > 0 ) this.mae_accu_day.Value = ACCU_DAY;
		if( length(ACCU_NUM) > 0 ) this.mae_accu_num.Value = ACCU_NUM;
		if( length(ACCU_BRAN) > 0) this.edt_accu_bran.Text = ACCU_BRAN;
		
		if ( length(ACCU_DAY) > 0 ) {
			fn_getAccuList();
		}
	}
}

function fn_CallBack(srvId,ErrorCode,ErrorMsg)
{
	if(ErrorCode < 0) {
		//에러 공통 팝업
		gfn_SetErrorPop(ErrorCode,ErrorMsg);
	}
}

function btn_Close_OnClick(obj)
{
	Close();
}

function fn_getAccuList()
{
	if (this.cob_BRAN_CD.Value == null) {
        gfn_Confirm("MB_OK", "센터를 선택해주세요.", "경고", "0");
		this.cob_GIBU.SetFocus();
		return;
	}
/*
	if (this.cob_accu_gbn.Value == null) {
        gfn_Confirm("MB_OK", "고소구분을 선택하세요.", "경고", "0");
		this.cob_accu_gbn.SetFocus();
		return;
	}
*/
	ds_accu_search.ClearData();
	ds_accu_search.AddRow();
	
	ds_accu_search.SetColumn(0, "BRAN_CD"    , this.cob_BRAN_CD.Value);
	ds_accu_search.SetColumn(0, "ACCU_DAY"   , this.mae_accu_day.Value);
	ds_accu_search.SetColumn(0, "ACCU_NUM"   , this.mae_accu_num.Value);
	ds_accu_search.SetColumn(0, "ACCU_BRAN"  , this.edt_accu_bran.Text);
	ds_accu_search.SetColumn(0, "ACCU_GBN"   , this.cob_accu_gbn.Value);
	ds_accu_search.SetColumn(0, "UPSO_NM"    , this.edt_UPSO_NM.Text);
	ds_accu_search.SetColumn(0, "MNGEMSTR_NM", this.edt_MNGEMSTR_NM.Text);
	ds_accu_search.SetColumn(0, "PERMMSTR_NM", this.edt_PERMMSTR_NM.Text);

	gfn_syncCall("find_accu_no","KOMCA?SYSID=PATHFINDER&MENUID=1000001006002001&EVENTID=p_accu_list","S=ds_accu_search","ds_accu_list=SEL1","","fn_CallBack");
	
}

function btn_Search_OnClick(obj)
{
	fn_getAccuList();
}

function edt_UPSO_NM_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar==13){
		fn_getAccuList();
	}
}

function edt_MNGEMSTR_NM_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar==13){
		fn_getAccuList();
	}
}
function edt_PERMMSTR_NM_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar==13){
		fn_getAccuList();
	}
}
// 조회된 고소리슽에서 더블클릭시
function grd_accu_list_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	var ACCU_DAY  = this.ds_accu_list.GetColumn(nRow,"ACCU_DAY");
	var ACCU_NUM  = this.ds_accu_list.GetColumn(nRow,"ACCU_NUM");
	var ACCU_BRAN = this.ds_accu_list.GetColumn(nRow,"ACCU_BRAN");
	var ACCU_GBN  = this.ds_accu_list.GetColumn(nRow,"ACCU_GBN");

	if (SGBN == "bra04_r04") {
		Close(ACCU_DAY + "," + ACCU_NUM + "," + ACCU_BRAN + "," + ACCU_GBN);
	}
	else if (SGBN == "bra05_s03") {
		parent.cob_BRAN_CD.Value = this.cob_BRAN_CD.Value;
		parent.cob_accu_gbn.Value = this.cob_accu_gbn.Value;
		parent.mae_accu_day.Value = ACCU_DAY;	
		parent.mae_accu_num.Value = ACCU_NUM;	
		parent.edt_accu_bran.Value = ACCU_BRAN;	
	
		parent.fn_Search_m();
		close();
	}
}




