function bra08_s09_OnLoadCompleted(obj)
{	gfn_LoadForm(obj);
	
	this.div0.cal_start.Value = addMonth(today(), -3);
	this.div0.cal_end.value = today();
	
	gfn_PubCode('ds_open_close_gbn', '1', '00433', 'OPEN');
	gfn_PubCode('ds_stat_gbn', '2', '00433', 'STAT');
	
	trace(ds_open_close_gbn.SaveXML());
	trace(ds_stat_gbn.SaveXML());
	
	this.Div0.cob_gbn.value = "%";
		// 시도 정보 검색
	gfn_syncCall("LOAD_ATTE","KOMCA?SYSID=PATHFINDER&MENUID=1000001006007&EVENTID=p_popup_sigudong","","ds_ATTE=SEL1","","fn_CallBack");
}


//	공통버튼 컨트롤
function fn_CommmonButtonClick(strCode){
    //신규
    if(strCode == "NEW"){}
    //조회
    else if(strCode == "SEARCH"){
		fn_Search();
    }
    //저장
    else if(strCode == "SAVE"){
		fn_Save();
    }    
    //삭제
    else if(strCode == "DELETE"){}
    //Excel
    else if(strCode == "EXCEL"){
		fn_Excel();
    }
    //출력
    else if(strCode == "PRINT"){}
    //닫기
    else if(strCode == "CLOSE"){
        close();
    }	return;}

//조회
function fn_Search(){
    if (!gfn_CheckFormNull(Div0)) return;
    
    this.ds_search.SetColumn(0, "START_YRMN", substr(this.div0.cal_start.Value, 0,6));
    this.ds_search.SetColumn(0, "END_YRMN", substr(this.div0.cal_end.Value, 0,6));
    this.ds_search.SetColumn(0, "GBN", this.div0.cob_gbn.value);
    
    if (this.div0.cob_ATTE.text == "" ) {
		this.ds_search.SetColumn(0, "SI_GUN_GU", "");
    } else if (this.div0.cob_ATTE.text == "세종특별자치시" ) {
		this.ds_search.SetColumn(0, "SI_GUN_GU", this.div0.cob_ATTE.value);
    } else {
		this.ds_search.SetColumn(0, "SI_GUN_GU", this.div0.cob_ATTE.value + " " + this.div0.cob_DSRCCNTY.text);
	}
    
    if (this.tab_Onwer.TabIndex == 0) { 	//노래방 일때
		this.ds_no_list1.ClearData();
		this.ds_no_list2.ClearData();
		//trace(this.ds_search.SaveXML());
		gfn_syncCall("get_gov_data","KOMCA?SYSID=PATHFINDER&MENUID=1000001006008009&EVENTID=get_gov_data","S=ds_search","ds_no_list1=MRG6","","fn_CallBack");
		//trace(this.ds_no_list1.SaveXML());
	} else if (this.tab_Onwer.TabIndex == 1) {	//단란 일때
		this.ds_dan_list1.ClearData();
		this.ds_dan_list2.ClearData();
		//trace(this.ds_search.SaveXML());
		gfn_syncCall("get_gov_danran","KOMCA?SYSID=PATHFINDER&MENUID=1000001006008009&EVENTID=get_gov_danran","S=ds_search","ds_dan_list1=MRG6","","fn_CallBack");
		//trace(this.ds_dan_list1.SaveXML());
	} else if (this.tab_Onwer.TabIndex == 2) {	//유흥 일때
		this.ds_yu_list1.ClearData();
		this.ds_yu_list2.ClearData();
		//trace(this.ds_search.SaveXML());
		gfn_syncCall("get_gov_yuheong","KOMCA?SYSID=PATHFINDER&MENUID=1000001006008009&EVENTID=get_gov_yuheong","S=ds_search","ds_yu_list1=MRG6","","fn_CallBack");
		//trace(this.ds_yu_list1.SaveXML());
	}
}

//저장
function fn_Save() {
	
	if (this.tab_Onwer.TabIndex == 0) { 	//노래방 일때
		gfn_syncCall("save_stat_bigo","KOMCA?SYSID=PATHFINDER&MENUID=1000001006008009&EVENTID=save_stat_bigo","S=ds_no_list1:U","","","fn_CallBack", gv_TraceLog, gv_formid);
	} else if (this.tab_Onwer.TabIndex == 1) { 	//단란 일때
		gfn_syncCall("save_stat_bigo_danran","KOMCA?SYSID=PATHFINDER&MENUID=1000001006008009&EVENTID=save_stat_bigo_danran","S=ds_dan_list1:U","","","fn_CallBack", gv_TraceLog, gv_formid);
	} else if (this.tab_Onwer.TabIndex == 2) { 	//유흥 일때
		gfn_syncCall("save_stat_bigo_yuheong","KOMCA?SYSID=PATHFINDER&MENUID=1000001006008009&EVENTID=save_stat_bigo_yuheong","S=ds_yu_list1:U","","","fn_CallBack", gv_TraceLog, gv_formid);
	}
	
    fn_Search();
}


//엑셀출력
function fn_Excel(){
	if (this.tab_Onwer.TabIndex == 0) { 	//노래방 일때
		this.tab_Onwer.NOREBANG.grd_no_result1.ExportExcelEx("정부3.0데이터노래방_개폐업");
	} else if (this.tab_Onwer.TabIndex == 1) { 	//단란 일때
		this.tab_Onwer.DANRAN.grd_dan_result1.ExportExcelEx("정부3.0데이터단란_개폐업");
	} else if (this.tab_Onwer.TabIndex == 2) { 	//유흥 일때
		this.tab_Onwer.YUHEONG.grd_yu_result1.ExportExcelEx("정부3.0데이터유흥_개폐업");
	}
}


