function set_filter(){
	var gbn1 = this.Div0.cob_bscon_gbn.Value;
	
	if (gbn1 == "%") {
		if (this.div0.cob_statistics.value == "1") {
			this.ds_list1.UnFilter();
		} else {
			this.ds_list2.UnFilter();
		}
	} else {	//반주기정보를 태진 혹은 금영을 선택했을때.
		if (this.div0.cob_statistics.value == "1") {
			this.ds_list1.Filter("BSCON_CD='"+ gbn1+"'");
		} else {
			this.ds_list2.Filter("BSCON_CD='"+ gbn1+"'");
		}
	}
}

function Div0_cob_bscon_gbn_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	set_filter();
}
