function fn_Excel()
{
	var fileNm = "";
	
	if(cob_BRAN_CD.Index == 0)
	{
		fileNm = "전체";
	}
	else
	{
		fileNm = cob_BRAN_CD.Text;
	}
		
	if(tab_cndtn.TabIndex == 0)
	{
		fileNm = cal_Sday.Value + "_" + fileNm + "_출장현황";
		
		tab_cndtn.visit_list.grd_visit_cndtn.ExportExcelEx(fileNm);
	}
	else if(tab_cndtn.TabIndex == 1)
	{
		fileNm = cal_Sday.Value + "_" + fileNm + "_업무통계";
		
		tab_cndtn.work_statistic.grd_work_stat.ExportExcelEx(fileNm);
	}
	else if(tab_cndtn.TabIndex == 2)
	{
		fileNm = cal_Sday.Value + "_" + fileNm + "_업무상세";
		
		tab_cndtn.work_statistic_desc.grd_work_desc.ExportExcelEx(fileNm);
	}
	else if(tab_cndtn.TabIndex == 3)
	{
		fileNm = cal_Sday.Value + "_" + fileNm + "_근태현황";
		
		tab_cndtn.duty_list.grd_duty_list.ExportExcelEx(fileNm);
	}
}

