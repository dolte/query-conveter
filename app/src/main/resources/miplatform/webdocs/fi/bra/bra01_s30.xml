function bra01_s30_OnLoadCompleted(obj)
{
	gfn_LoadForm(obj);
	
	bra01_s30_OnSize();

	fn_new();
	
	gfn_GetDataGIBU_CD(this.cob_BRAN_CD, 2);
	cob_BRAN_CD_OnChanged();
	
	gfn_getComboCode("1000002001001003","cb_staff_cd","ds_code_staff");   //  사원정보 가져오기

	
	gfn_syncCall("svcAuth","KOMCA?SYSID=PATHFINDER&MENUID=1000001006008008&EVENTID=sel_udtkpres","","ds_Auth=SEL1","","fn_CallBack");
	
	
	if(gds_sessioninfo.GetColumn(0, "DEPT_CD") == "122040100" || gds_sessioninfo.GetColumn(0, "DEPT_CD") == "121020000")
	{//센터관리, 전산팀 권한
			
	}
	else 
	{
		var objBtn;
		var comps = this.parent.dvtitle.divIcon.Components["Button"];
		for( var i = 0 ; i < comps.Count ; i++)
		{
			objBtn = object("dvtitle.divIcon." + comps[i].id);
			if (objBtn.UserData == "SAVE" ) {
				objBtn.Enable = false;
			}
		}
		
		if( ds_AUTH.FindRow("UDTKPRES_ID",gds_sessioninfo.GetColumn(0,"USER_ID"))>=0)
		{//센터장 권한
		//grd_List.SetColProp(22, "Width", "0");
		}
		else{ //일반직원
			cob_staff_nm.Enable=false;
			btn_conf.Enable = false;
		}
	}
}

function fn_CommmonButtonClick(strCode)
{
    if(strCode == "NEW"){
    ///신규    
		fn_new();
    }else if(strCode == "SEARCH"){
		fn_search();    
    ///조회
    }else if(strCode == "SAVE"){
		fn_save();
    ///저장
    }else if(strCode == "EXCEL"){
    ///엑셀저장
		fn_excel();
    }else if(strCode == "DELETE"){
    ///삭제
    }else if(strCode == "PRINT"){
    ///출력
    }else if(strCode == "CLOSE"){
    ///닫기
    close();
    }
}

function fn_new() {
	
	ds_List.ClearData();
	ds_search.ClearData();
	cal_StartDt.Enable =false;
	cal_EndDt.Enable=false;
	
	btn_n_search.Enable=false;

}


function fn_save() {
	//센터관리팀만 할인시작,종료,적용 저장 가능
	gfn_syncCall("svcSave","KOMCA?SYSID=PATHFINDER&MENUID=1000001006004032&EVENTID=router_dsct_upd","S=ds_List:U","","","fn_CallBack");
	
}


function fn_excel()
{
	if(ds_List.rowcount < 1)
	{
		alert("조회된 내역이 없습니다.");
		return;
	}
	
	grd_List.ExportExcelEx("오프라인 로그데이터 통합 관리");

}

function fn_search() {
	
	if(length(cob_BRAN_CD.Value)==0)
	{
		alert("센터명을 입력하세요");
		cob_BRAN_CD.SetFocus();
		return;
	}
	
	ds_search.AddRow();
	
	ds_search.SetColumn(0,"BRAN_CD",cob_BRAN_CD.Value);
	ds_search.SetColumn(0,"STAFF_CD",cob_Staff_Nm.Value);
	ds_search.SetColumn(0,"PROC_STS",cob_proc.Value);

	gfn_syncCall("svcSearch","KOMCA?SYSID=PATHFINDER&MENUID=1000001006004032&EVENTID=router_search","S=ds_search","ds_List=SEL1","","fn_CallBack");
	
	rad_sort_OnClick();
	//trace(ds_List.SaveXML());

}

/*
 * 기      능: Callback함수
 * 인      수: Argument	설명
 * Return    : Return Value	
 * 예     시 : 함수 사용 예	
 */
function fn_CallBack(svcid,errCode,errMsg) {
	
	if (errCode = 0)
	{
		switch (svcid)
		{
			case "svcSearch" :
				cal_StartDt.Enable =true;
				cal_EndDt.Enable=true;
				btn_n_search.Enable=true;

				break;
			case "svcDel" :
				fn_search();
				alert("삭제 완료되었습니다.");

				break;
				
			case "svcSave" :
				fn_search();
				alert("저장이 완료되었습니다.");

				break;				
				
			default : break;			
		}		
	}
}

//센터코드 변경시 해당 담당사원 조회
function cob_BRAN_CD_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	gfn_SetPermission(cob_BRAN_CD.Value);

	ds_BRAN_CD.SetColumn(0, "BRAN_CD", cob_BRAN_CD.Value);
	gfn_syncCall("SAWON_CLICK","KOMCA?SYSID=PATHFINDER&MENUID=1000001006007&EVENTID=p_staff_select","S=ds_BRAN_CD","ds_SawonInfo=SEL1","","fn_CallBack");

	var iRow = ds_SawonInfo.InsertRow(0);
	ds_SawonInfo.SetColumn(iRow,"STAFF_CD","");
	ds_SawonInfo.SetColumn(iRow,"STAFF_NM","ALL");
	
	cob_Staff_Nm.Value = gds_sessioninfo.GetColumn(0, "STAFF_NUM");
	if(cob_Staff_Nm.Index < 0)
	{
		cob_Staff_Nm.Index = 0;
	}	
}

function common_pop_OnClick(obj)
{	
	//gfn_OpenChildWindow("fi_bra::pop_bra01_s30.xml","오프라인 로그데이터 이력 관리","");
	
	if(ds_List.rowcount<=0)
	{
		alert("조회후 이용가능합니다.");
		return;
	}

	if( ds_List.GetColumn(ds_List.currow,"PROC_STS") <>"M" &&
		 ds_List.GetColumn(ds_List.currow,"PROC_STS")<>"N")
	{
		alert("매칭, 변경신청 중인 것들만 수정 가능합니다.");
		return;
	}

	// 결재
	var result = Dialog("fi_bra::pop_bra01_s30.xml","","300","300","",-1,-1);
	fn_search();
	
}

function btn_can_OnClick(obj)
{// 회수
	
	if(ds_list.GetColumn(ds_list.currow, "PROC_STS")=="M")
	{
		alert("변경신청 중인 업소만 회수가 가능합니다.");
		return;
	}
	
	if(Confirm("입력중인 " + ds_list.GetColumn(ds_list.currow, "UPSO_CD") + " 라우터 신청을 삭제하시겠습니까?"))
	{
		ds_del.ClearData();
		ds_del.AddRow();
		ds_del.SetColumn(0,"SERIAL_NO",ds_list.GetColumn(ds_list.currow, "SERIAL_NO") );
		
		gfn_syncCall("svcDel","KOMCA?SYSID=PATHFINDER&MENUID=1000001006004032&EVENTID=router_n_del","S=ds_del","","","fn_CallBack");
	}
		
}

function grd_List_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if(nCell ==12)
	{
		if(grd_List.GetCellValue(nRow,"12")==0)
		{
			alert("다운로드할 파일이 없습니다.");
			return;
		}
		
		ds_search_file.ClearData();
		ds_search_file.AddRow();
		
		var upso_cd = grd_List.GetCellValue(nRow,"2");
		var serial_no = ds_List.GetColumn(ds_List.currow,"SERIAL_NO");
		var serial_no_seq = ds_List.GetColumn(ds_List.currow,"SERIAL_NO_SEQ");
		
		if(ds_List.GetColumn(ds_List.currow,"PROC_STS")="N")
		{
			serial_no_seq=serial_no_seq+1;
		}
					
		ds_search_file.SetColumn(0,"UPSO_CD",upso_cd);
		ds_search_file.SetColumn(0,"SERIAL_NO",serial_no);
		ds_search_file.SetColumn(0,"SERIAL_NO_SEQ",serial_no_seq);
		
		gfn_syncCall("svcdown","KOMCA?SYSID=PATHFINDER&MENUID=1000001006004032&EVENTID=router_file_search","S=ds_search_file","ds_file=SEL1","","fn_CallBack");
		
		if(ds_file.rowcount<=0)
		{
			alert("다운로드할 파일이 없습니다.");
			return;
		}
		
		var rout = ds_file.GetColumn(0, "SVR_FILE_ROUT");
		var file = ds_file.GetColumn(0, "SVR_FILE_NM");
		var org  = ds_file.GetColumn(0, "FILE_NM");
	
		gfn_fileDownLoad(rout, file, File0, FileDialog0, org);
	
	}
	
}	

function bra01_s30_OnSize(obj,nCx,nCy,nState)
{
	gfn_ObjreSize(grd_List, this, 1, 0);
}



function btn_n_search_OnClick(obj)
{
	if(ds_List.rowcount<=0)
	{
		alert("조회후 이용할수 있습니다.");
		return;
	}

	if(cal_StartDt.Value >cal_EndDt.Value)
	{
		alert("시작일자가 종료일자보다 클수 없습니다.");
		return;
	}
		
		
	ds_List.Filter("LAST_REC_DAY <='"+cal_EndDt.Value+"'");
}


function grd_List_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	g_bran_cd	= ds_List.GetColumn(nRow, "BRAN_CD");
	g_upso_cd	= ds_List.GetColumn(nRow, "UPSO_CD");
    gfn_OpenChildWindow("fi_bra::bra01_s01.xml", "업소 등록","");
	
}

function edt_upso_cd_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if (nChar=13)
	{
		if (edt_upso_cd.Value= "")
		{
			ds_List.Filter("UPSO_CD IS NOT NULL");
		}
		else
		{
			ds_List.Filter("UPSO_CD ='"+edt_upso_cd.Value+"'");
		}
	}
}

function rad_sort_OnClick(obj)
{ 
	
	if(rad_sort.Value==0)
	 {
		ds_List.Filter("REP_OBJ >=0");
	 }
	 else if(rad_sort.Value==4)
	 {
		ds_List.Filter("PROC_STS='N'");
	 }
	 else 
	 {
		ds_List.Filter("REP_OBJ ='"+rad_sort.Value+"'");
	}
}
