function btn_FindUpso_OnClick(obj)
{
	//업소코드 입력 조회시 바로 조회창 페이지를 거치지 않고 바로 검색
	if(this.cob_BRAN_CD.Value==null){
	
        gfn_Confirm("MB_OK", "센터를 선택해주세요.", "경고", "0");
		this.cob_BRAN_CD.SetFocus();
		return false;
	}
	
	fn_FindUPSO();
}

function fn_FindUPSO()
{
	var BRAN_CD = this.cob_BRAN_CD.Value;
	var upso_cd;
	if(this.edt_UPSO_NM.Text == ""){
		UPSO_CD = this.edt_UPSO_CD.Text;
	}
	else{
		UPSO_CD = "";
	}
	
	this.edt_UPSO_NM.SetFocus();
	var UPSO_NM = this.edt_UPSO_NM.Text;
	var inParam ="BRAN_CD=" + quote(BRAN_CD) + " UPSO_CD=" + quote(UPSO_CD) + " UPSO_NM=" + quote(UPSO_NM) + " SGBN=" + quote("bra01_s01");
	var result = Dialog("fi_bra::bra10_p21.xml",inParam);

	if(0 < length(result)){
	    var vArr = result.split(",");
	    this.edt_UPSO_CD.Text = vArr[0];
	    this.edt_UPSO_NM.Text = vArr[1];
	}
}

function edt_UPSO_CD_OnChar(obj,strPreText,nChar,strPostText,LLParam,HLParam)
{
	var strtext = right(strPostText,1);
	var chkstr = "[<!#$%*&_:=|`~'"+Quote("")+";@^+\\>-]"; 
	
	if(pos(chkstr, strtext) >= 0)   // 특수문자 사용 제한
	{
		return false;
	}
	
	//if (length(strPostText) == 8 && nChar >= asc('a') && nChar <= asc('z')) {
	if (nChar >= asc('a') && nChar <= asc('z')) {
		obj.Text = strPreText + toUpper(chr(nChar));
		edt_UPSO_CD.SetSel(length(edt_UPSO_CD.Text));
		return false;
	}
}

function edt_UPSO_CD_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	//업소코드 입력 조회시 바로 조회창 페이지를 거치지 않고 바로 검색
	if(this.cob_BRAN_CD.Value==null){
        gfn_Confirm("MB_OK", "센터를 선택해주세요.", "경고", "0");
		this.cob_BRAN_CD.SetFocus();
		return false;
	}
	
	if (nChar == "13") {
		//fn_GetUPSO_CD_cd();
		fn_FindUPSO();
	}
	else if (nChar == "112") fn_FindUPSO();
}

function fn_GetUPSO_CD_cd()
{
	var BRAN_CD = this.cob_BRAN_CD.Value;
	var UPSO_CD = this.edt_UPSO_CD.Text;
	var UPSO_NM = "";
	var result = gfn_GetUPSO_CD(BRAN_CD, UPSO_CD, UPSO_NM);

	if (length(result) > 0) {
		var vArr = result.split(",");
		this.edt_UPSO_CD.Text = vArr[0];
		this.edt_UPSO_NM.Text = vArr[1];
		fn_search();
	}
	else {
		fn_FindUPSO();
	}
}

function edt_UPSO_NM_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	//업소코드 입력 조회시 바로 조회창 페이지를 거치지 않고 바로 검색
	if(this.cob_BRAN_CD.Value==null){
	
        gfn_Confirm("MB_OK", "센터를 선택해주세요.", "경고", "0");
		this.cob_BRAN_CD.SetFocus();
		return false;
	}
	if (nChar == "112" || nChar == "13") {
		fn_FindUPSO();
	}

	return;
}

function btn_upload_OnClick(obj)
{
	if(cob_BUSINESS_LIST.Index == 0)
	{
		alert("매장음악사업자 선택 후 업로드해주시기 바랍니다.");
		return;
	}
	
	if(FileDialog.Open() == 0)
	{
		return;
	}
	
	ds_search.SetColumn(0, "REC_YRMN", "");
	
	if(!Confirm(substr(cal_yrmn.Value, 0, 6) + " 계약정보를 저장하시겠습니까?"))
	{
		return;
	}
	
	//매장음악사업자명_정산년월.확장자   에서 정산년월만 불러오기
	//var vUpDay = substr(FileDialog.FileName, indexOf(FileDialog.FileName, "_") + 1, length(FileDialog.FileName) - indexOf(FileDialog.FileName, ".") - 1);
	var vUpDay = substr(FileDialog.FileName, indexOf(FileDialog.FileName, "_") + 1, 8);
	
	ds_search.SetColumn(0, "UPLOD_DAY", vUpDay);
	
	gfn_syncCall("svcDupCheck","KOMCA?SYSID=PATHFINDER&MENUID=1000001006013005&EVENTID=chk_stomu_rept","S=ds_search", "ds_dup_cnt=SEL1", "", "fn_CallBack");
	
	if(parseInt(ds_dup_cnt.GetColumn(0, "CNT")) > 0)
	{
		if(!Confirm(vUpDay + " 일자로 등록된 입금내역이 있습니다.\n삭제 후 등록하시겠습니까?"))
		{
			return;
		}
		
		gfn_syncCall("svcDelete","KOMCA?SYSID=PATHFINDER&MENUID=1000001006013005&EVENTID=del_stomu_rept","S=ds_search", "", "", "fn_CallBack");
	}

	ds_excel.ClearData();
	
	fn_view_setting("E");
	
	ext_ExcelImportByIndex(FileDialog.FilePath + "\\" + FileDialog.FileName, 0, "ds_excel", 1, 1, 0, 2);
}

function cob_search_gbn_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	fn_view_setting(cob_search_gbn.Value);
}

function fn_view_setting(vFlag)
{
	grd_rept_list.Visible = true;
	grd_excel.Visible = false;
	
	if(vFlag == "D")
	{
		Image9.Visible = false;
		Static9.Visible = false;
		Shape1.Visible = false;
		grd_rept_statistic.Visible = false;
		grd_rept_list.Height = 696;
	}
	else if(vFlag == "R")
	{
		Image9.Visible = true;
		Static9.Visible = true;
		Shape1.Visible = true;
		grd_rept_statistic.Visible = true;
		grd_rept_list.Height = 544;
	}
	else if(vFlag == "E")
	{
		grd_rept_list.Visible = false;
		Image9.Visible = false;
		Static9.Visible = false;
		Shape1.Visible = false;
		grd_rept_statistic.Visible = false;
		
		grd_excel.Visible = true;
	}
}

function cal_OnChanged(obj,strOldText,strNewText)
{
	if(obj == cal_start_yrmn && cal_start_yrmn.Value > cal_end_yrmn.Value)
	{
		cal_end_yrmn.Value = cal_start_yrmn.Value;
	}
	else if(obj == cal_end_yrmn && cal_start_yrmn.Value > cal_end_yrmn.Value)
	{
		cal_start_yrmn.Value = cal_end_yrmn.Value;
	}
}
