{
	var yn;
	
	if (nCell == 14)
	{
		checked = !checked;
		
		if (checked = true)
		{
			yn = '1';
		}
		else
		{
			yn = '0';
		}

		for (var i = 0; i < this.ds_ClseUpso.rowcount; i++)
		{
			if(ds_ClseUpso.GetColumn(i,"SATN_YN") != yn)
			{
				this.ds_ClseUpso.SetColumn(i, "SATN_YN", yn);
			}
		}
	}
}

