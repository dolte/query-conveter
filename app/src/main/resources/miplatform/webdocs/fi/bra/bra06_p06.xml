	}
}

function fn_Search()
{	
	s_nonpy_amt=0;	//미납금의 개수
	s_show_goso=0;	//고소미해결 업소수
	s_show_goso_amt=0;	//고소미해결금액
	
	// 입력 필수 항목 검사;
	if (!gfn_CheckFormNull(this)) return;
	
	this.ds_In.ClearData();
	this.ds_In.AddRow();
	
	this.ds_In.setcolumn(0,"START_YRMN", this.cal_start.Value);
	this.ds_In.setcolumn(0,"END_YRMN", this.cal_end.Value);
	this.ds_In.SetColumn(0,"BRAN_CD", this.cob_BRAN_CD.Value);
	this.ds_In.SetColumn(0,"GBN1", this.cob_order_gbn1.Value);
	this.ds_In.SetColumn(0,"GBN2", this.cob_order_gbn2.Value);
	this.ds_In.SetColumn(0,"REAS_GBN", cob_reas_gbn.Value);
	
	if(this.tab.TabIndex == 0)
	{
		ds_In.setcolumn(0,"START_YRMN", substr(cal_start.Value, 0, 6));
		ds_In.setcolumn(0,"END_YRMN", substr(cal_end.Value, 0, 6));
	
		this.ds_In.SetColumn(0,"GBN", "1");
		gfn_syncCall("sync_upso_list","KOMCA?SYSID=PATHFINDER&MENUID=1000001006009009&EVENTID=upso_list","S=ds_In","ds_NewUpso=SEL1 ds_NewUpso_1=SEL2","","");
	}
	else if(this.tab.TabIndex == 1)
	{
		this.ds_In.SetColumn(0,"GBN", "4");
		gfn_syncCall("sync_upso_list","KOMCA?SYSID=PATHFINDER&MENUID=1000001006009009&EVENTID=upso_list","S=ds_In","ds_InsertUpso=SEL4","","");
	}
	else if(this.tab.TabIndex == 2) // 휴업업소
	{
		this.ds_In.SetColumn(0,"GBN", "2");
		gfn_syncCall("sync_CLseUpso","KOMCA?SYSID=PATHFINDER&MENUID=1000001006009009&EVENTID=upso_list","S=ds_In","ds_ClseUpso=SEL3","","");
	}
	else // 폐업업소
	{
		this.ds_In.SetColumn(0,"GBN", "3");
		gfn_syncCall("sync_PaeUpso","KOMCA?SYSID=PATHFINDER&MENUID=1000001006009009&EVENTID=upso_list","S=ds_In","ds_PaeUpso=SEL7","","");

		for( var i=0; i<this.ds_PaeUpso.RowCount(); i++)
		{
			if(this.ds_PaeUpso.GetColumn(i, "ACCU_CHK") == 1)
			{
				s_show_goso = s_show_goso+1;
				s_show_goso_amt = toNumber(s_show_goso_amt) + toNumber(this.ds_PaeUpso.GetColumn(i, "TOT_AMT"));
			}
			
			if(this.ds_PaeUpso.GetColumn(i,"NONPY_AMT") >0)
			{
				s_nonpy_amt = s_nonpy_amt+1;		
			}
		}
		
	}
	//btn_sort_OnClick();
}

function fn_Save()
{
	if(this.tab.TabIndex == 0)
	{
		alert("신규입금업소는 저장기능이 없습니다.");
	}
	else if(this.tab.TabIndex == 1)
	{
		alert("신규등록업소는 저장기능이 없습니다.");
	}
	else if(this.tab.TabIndex == 2)
	{
		//trace(this.ds_ClseUpso.SaveXML());
		gfn_syncCall("save1","KOMCA?SYSID=PATHFINDER&MENUID=1000001006009009&EVENTID=save_clsed_satn","S=ds_ClseUpso:U","","","fn_CallBack", gv_TraceLog, gv_formid);
		fn_Search();
	}
	else 
	{
		//trace(this.ds_PaeUpso.SaveXML());
		gfn_syncCall("save1","KOMCA?SYSID=PATHFINDER&MENUID=1000001006009009&EVENTID=save_clsed_satn","S=ds_PaeUpso:U","","","fn_CallBack", gv_TraceLog, gv_formid);
		fn_Search();
	}
}

