		dsName = ds_PaeUpso;
	}
	
	if(dsName.rowcount < 1)
	{
		alert("조회 후 이용하세요.");
		return;
	}
	
	ubiviewer0.Visible = true;
	btn_Report_Close.Visible = true;
	tab.Enable = false;
	
	ubiviewer0.SetArgument("START_YRMN", this.cal_start.value);
	ubiviewer0.SetArgument("END_YRMN", this.cal_end.value);
	ubiviewer0.SetArgument("BRAN_CD", this.cob_BRAN_CD.Text);
	ubiviewer0.setJrfFileDir(gv_ReportUrl + "fi/bra/");
	ubiviewer0.Exportsetmode = 1;
	ubiviewer0.SetVariable("usePDF2", "true");
	ubiviewer0.FontRevision = true;
	
	ubiviewer0.Retrieve();
	//ubiviewer0.directPrint();
}

