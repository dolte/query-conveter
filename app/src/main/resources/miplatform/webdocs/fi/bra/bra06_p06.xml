		tab.tab_new.grd_new_upso_list.ExportExcelEX("신규입금업소내역서");
		tab.tab_new.grd_new_upso_list2.ExportExcelEX("신규입금업소내역서 요약");
	}
	else if(this.tab.TabIndex == 1)
	{
		if(ds_InsertUpso.rowcount < 1)
			return;
		tab.tab_insert.grd_insert_upso_list.ExportExcelEX("신규등록업소내역서");
	}
	else if(this.tab.TabIndex == 2)
	{
		if(ds_ClseUpso.rowcount < 1)
			return;
		tab.tab_close.grd_closed_upso_list.ExportExcelEX("휴업업소내역서");
