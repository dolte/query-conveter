		ubiviewer0.setJrfFileName("bra06_p06_2.jrf");
		ubiviewer0.SetDataset("sql_ClseUpso", "ds_ClseUpso");
		ubiviewer0.ReportTitle = cal_start.Value + "_" + cal_end.Value + "휴업업소내역서";
		
