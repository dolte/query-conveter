	}
	else
	{
		ubiviewer0.SetArgument("GBN","폐업");

		ubiviewer0.SetArgument("s_goso", s_show_goso);
		ubiviewer0.SetArgument("s_nonpy_amt", s_nonpy_amt);
		ubiviewer0.SetArgument("s_show_goso_amt", s_show_goso_amt);
		ubiviewer0.setJrfFileName("bra06_p06_3.jrf");
		ubiviewer0.SetDataset("sql_PaeUpso", "ds_PaeUpso");
